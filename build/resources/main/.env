# build environment one of enum values [ DEVELOPMENT, STAGING, PRODUCTION ] 
#BUID_ENVIRONMENT=STAGING
BUID_ENVIRONMENT=PRODUCTION
#BUID_ENVIRONMENT=DEVELOPMENT
# Application version
APP_VERSION=2.6.0

# Database Encrpytion Activation
SQLITE_ENCRYPTION_MODE=false

# Application Name
APPLICATION_NAME=WorkShiftly

# Application data directory name
APPLICATION_DATA_DIRECTORY=WorkShiftlyApp

# Development environment API
DEVELOPMENT_API=http://localhost:5000/api

# Staging environment API
#STAGING_API=http://173.230.129.87/api
STAGING_API=https://staging-api.workshiftly.com/api 
#STAGING_API=https://qa.workshiftly.com/api

# Production environment API
PRODUCTION_API=https://portal.workshiftly.com/api

# default threshold of persisting active window into database  
ACTIVE_WINDOW_WRITING_THERESHOLD=30

# maximum focus duration for an active window can be hold without persisting database
# unit should be minutes 
ACTIVE_WINDOW_MAX_FOCUS_DURATION=15

# configure that screenshot should be captured or not in user local file system
PERSIST_SCREEN_SHOTS_LOCAL_DISK=false

# Terms of condition html page url
#TERMS_OF_CONDITION_HTML_URL=https://tc-websites.s3.us-east-2.amazonaws.com/tc-ws-site/tc.html
TERMS_OF_CONDITION_HTML_URL=https://kn-workshiftly-html.s3.amazonaws.com/end_user_license_agreement.html

# Animate changing main screens of application
IS_ANIMATE_MAIN_SCREEN_CHANAGES=true

# Sentry DSN
SENTRY_DSN=https://6aac5effabc947059eadd407a143c755@o445376.ingest.sentry.io/5421686

# Time interval for periodic task - Synchronizing desktop-client's raw data to api(unit is minitues)
RAW_DATA_PERIODIC_TASK_INTERVAL=15
